package de.telekom.cldii.config;

import android.app.Activity;

/**
 * Listener for activity lifecycle events.
 * Replace with Application.ActivityLifecycleCallbacks if upgrading to API Level 14
 * 
 * @author Anton Wolf, jambit GmbH
 */
public interface IActivityLifecycleListener {

	void onActivityStarted(Activity activity);

	void onActivityStopped(Activity activity);
}
